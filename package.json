{
  "name": "globaltimepro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate:timezones": "node --experimental-specifier-resolution=node scripts/generate-timezones.mjs",
    "generate:sw-config": "node --experimental-specifier-resolution=node scripts/generate-sw-config.mjs",
    "generate:sitemap": "node --experimental-specifier-resolution=node scripts/generate-sitemap.mjs",
    "prebuild": "npm run generate:timezones && npm run generate:sw-config && npm run generate:sitemap",
    "build": "next build",
    "start": "next start",
    "predev": "npm run generate:timezones",
    "dev": "npm run generate:timezones && next dev",
    "dev:pages": "wrangler pages dev . --compatibility-date=2026-02-02",
    "lint": "next lint"
  },
  "dependencies": {
    "@vvo/tzdb": "^6.187.0",
    "city-timezones": "^1.3.2",
    "classnames": "^2.5.1",
    "countries-and-timezones": "^3.8.0",
    "fuse.js": "^7.1.0",
    "html2pdf.js": "^0.14.0",
    "jszip": "^3.10.1",
    "mailersend": "^2.6.0",
    "moment-timezone": "^0.6.0",
    "next": "^16.1.6",
    "react": "^19.0.0",
    "react-collapsed": "^4.2.0",
    "react-datetime-picker": "^6.0.1",
    "react-icons": "^5.4.0",
    "react-loading-skeleton": "^3.5.0",
    "react-responsive": "^10.0.0",
    "react-select": "^5.9.0",
    "react-switch": "^7.1.0",
    "react-toastify": "^11.0.2",
    "react-window": "^1.8.11",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "wrangler": "^4.61.1"
  }
}
